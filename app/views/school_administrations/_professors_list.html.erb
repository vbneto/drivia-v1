<div id='show_professor_list'>
  <h5>List of the parents of your school</h5>
  <table class="table table-striped table-bordered ">
    <thead>
    <tr>
      <th><%= t('school_administration.student_list.parent_name') %> </th>
      <th><%= t('school_administration.student_list.current_grade') %></th>
      <th>Subject Name</th>
      <th></th>
    </tr>
    </thead>
    <tbody>
      <% @professors.each do |professor| %>
        <tr>
          <td><%= link_to professor.professor_name, "#" %></td>
          <td><%= professor.grade_name %></td>
          <td><%= professor.subject_name %></td>
          <td>
            <%= link_to t('school_administration.student_list.expand'), 'javascript:void(0);', :class => "btn btn-primary"%>
            <%= link_to t("school_administration.student_list.edit"), "#", :class => "btn btn-primary" %>
          </td>  
        </tr>
      <%end%>
    </tbody>  
  </table>
</div>

